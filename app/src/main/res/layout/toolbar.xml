<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/MBlue"
    android:id="@+id/toolbarmain">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center">

    <ImageButton
        android:background="@drawable/homecon_foreground"
        android:id="@+id/homeIcon"
        android:layout_width="60dp"
        android:layout_height="60dp"/>
    <ImageButton
        android:background="@drawable/searchcon_foreground"
        android:id="@+id/searchIcon"
        android:layout_width="60dp"
        android:layout_height="60dp"/>
    <ImageButton
        android:background="@drawable/infocon_foreground"
        android:id="@+id/infoIcon"
        android:layout_width="60dp"
        android:layout_height="60dp"/>
    <ImageButton
        android:background="@drawable/exitcon_foreground"
        android:id="@+id/exitIcon"
        android:layout_width="60dp"
        android:layout_height="60dp"/>

    </LinearLayout>


</androidx.appcompat.widget.Toolbar>